<link rel="stylesheet" href="/error.css">

<div class="Error-page-container">
    <img src="59563731_9315310.svg" alt="Error Illustration">
    <p class="Error-page-p"><%=errorMessage%></p>
    <button class="Error-page-btn" onclick="goBack()">
        <i class="fa-solid fa-circle-left"></i> &nbsp; Back
    </button>
</div>

<script>
    function goBack() {
        if (document.referrer) {
            window.history.back();
        } else {
            window.location.href = "index.html"; // Redirect if no history exists
        }
    }
</script>